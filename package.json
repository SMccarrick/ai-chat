    "@storybook/svelte": "^7.0.0-beta.60",
    "@storybook/sveltekit": "^7.0.0-beta.60",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
